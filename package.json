{
  "name": "@nextcloud/eslint-config",
  "version": "9.0.0-rc.0",
  "description": "Eslint shared config for nextcloud apps and libraries",
  "keywords": [
    "eslint",
    "nextcloud",
    "config",
    "Vue"
  ],
  "homepage": "https://github.com/nextcloud-libraries/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/nextcloud-libraries/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextcloud-libraries/eslint-config.git"
  },
  "license": "AGPL-3.0-or-later",
  "author": "Nextcloud GmbH and Nextcloud contributors",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "dist/index.mjs",
  "files": [
    "AUTHORS.md",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "vite --mode production build",
    "lint": "npm run build && eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest run"
  },
  "dependencies": {
    "@eslint/json": "^0.11.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-vue": "^10.0.0",
    "fast-xml-parser": "^5.0.9",
    "globals": "^16.0.0",
    "semver": "^7.7.1",
    "sort-package-json": "^3.0.0",
    "typescript-eslint": "^8.28.0"
  },
  "devDependencies": {
    "@nextcloud/vite-config": "^2.3.2",
    "@types/node": "^22.13.14",
    "eslint": "^9.23.0",
    "memfs": "^4.17.0",
    "vite": "^6.2.3",
    "vitest": "^3.1.1"
  },
  "peerDependencies": {
    "eslint": ">=9"
  },
  "engines": {
    "node": "^20.9 || ^22"
  },
  "devEngines": {
    "packageManager": {
      "name": "npm",
      "version": "^10"
    },
    "runtime": {
      "name": "node",
      "version": "^22"
    }
  }
}

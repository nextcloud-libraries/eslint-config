{
  "name": "@nextcloud/eslint-config",
  "version": "9.0.0",
  "description": "Eslint shared config for nextcloud apps and libraries",
  "keywords": [
    "eslint",
    "nextcloud",
    "config",
    "Vue"
  ],
  "homepage": "https://github.com/nextcloud-libraries/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/nextcloud-libraries/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextcloud-libraries/eslint-config.git"
  },
  "license": "AGPL-3.0-or-later",
  "author": "Nextcloud GmbH and Nextcloud contributors",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "dist/index.mjs",
  "files": [
    "AUTHORS.md",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "vite --mode production build",
    "lint": "npm run build && eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest"
  },
  "dependencies": {
    "@eslint/json": "^0.10.0",
    "@stylistic/eslint-plugin": "^3.1.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.15.0",
    "sort-package-json": "^2.14.0",
    "typescript-eslint": "^8.24.0"
  },
  "devDependencies": {
    "@nextcloud/vite-config": "^2.3.1",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "vite": "^6.1.0"
  },
  "peerDependencies": {
    "eslint": ">=9"
  },
  "engines": {
    "node": "^22",
    "npm": "^10"
  }
}

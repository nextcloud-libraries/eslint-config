{
  "name": "@nextcloud/eslint-plugin",
  "version": "3.0.0-beta.1",
  "description": "Nextcloud lint plugin for ESLint",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "nextcloud"
  ],
  "homepage": "https://github.com/nextcloud-libraries/eslint-plugin",
  "repository": "git+https://github.com/nextcloud-libraries/eslint-plugin.git",
  "license": "MIT",
  "author": "Christoph Wurst",
  "files": [
    "docs",
    "dist",
    "CHANGELOG.md"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "vite --mode production build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest run --watch"
  },
  "dependencies": {
    "fast-xml-parser": "^4.5.1",
    "semver": "^7.7.1"
  },
  "devDependencies": {
    "@nextcloud/vite-config": "^2.2.2",
    "eslint": "^9.14.0",
    "jest": "^29.7.0",
    "memfs": "^4.17.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "engines": {
    "node": "^20.0.0",
    "npm": "^10.0.0"
  }
}
